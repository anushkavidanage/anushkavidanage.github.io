{
  "@context": "https://www.w3.org/ns/solid/oidc-context.jsonld",
  "client_id": "https://anushkavidanage.github.io/solid_auth/example/client-profile.jsonld",
  "client_name": "Solid Auth Example App",
  "application_type": "native",
  "redirect_uris": [
    "http://127.0.0.1/redirect.html",
    "com.togaware.exampleapp://redirect"
  ],
  "post_logout_redirect_uris": [
    "http://127.0.0.1/redirect.html",
    "com.togaware.exampleapp://logout"
  ],
  "scope": "openid profile offline_access webid",
  "grant_types": [
    "authorization_code",
    "refresh_token"
  ],
  "response_types": [
    "code"
  ],
  "token_endpoint_auth_method": "none"
}